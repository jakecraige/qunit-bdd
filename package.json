{
  "name": "qunit-bdd",
  "version": "0.1.3",
  "description": "BDD-style testing for QUnit.",
  "main": "lib/qunit-bdd.js",
  "directories": {
    "test": "test"
  },
  "files": [
    "lib/qunit-bdd.js",
    "ember-addon/qunit-bdd.js",
    "LICENSE"
  ],
  "scripts": {
    "test": "node_modules/karma/bin/karma --singleRun=true start karma.conf.js",
    "develop": "node_modules/karma/bin/karma start karma.conf.js"
  },
  "keywords": [
    "qunit",
    "bdd",
    "ember-addon"
  ],
  "ember-addon": {
    "main": "ember-addon/qunit-bdd.js",
    "after": "ember-cli-qunit"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:square/qunit-bdd.git"
  },
  "author": "Brian Donovan",
  "license": "Apache 2",
  "devDependencies": {
    "qunitjs": "~1.12.0"
  }
}
